{
  "extends": ["next/core-web-vitals", "next/typescript", "prettier"],
  "plugins": ["import"],
  "rules": {
    "indent": ["error", 2], // 2칸 들여쓰기
    "import/order": [
      "error",
      {
        "groups": ["builtin", "external", "internal", "parent", "sibling", "index"],
        "newlines-between": "always",
        "alphabetize": {
          "order": "asc",
          "caseInsensitive": true
        }
      }
    ]
  }
}
